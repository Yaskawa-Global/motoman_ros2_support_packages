<?xml version="1.0" ?>
<robot xmlns:xacro="http://ros.org/wiki/xacro">
<xacro:macro name="motoman_gp225" params="prefix">

    <xacro:include filename="$(find motoman_resources)/urdf/common_materials.xacro"/>

    <!-- link list -->
    <link name="${prefix}base_link">
        <visual>
            <geometry>
                <mesh filename="package://motoman_gp225_support/meshes/gp225/visual/base_link.dae"/>
            </geometry>
            <xacro:material_yaskawa_blue/>
        </visual>
        <collision>
            <geometry>
                <mesh filename="package://motoman_gp225_support/meshes/gp225/collision/base_link.stl"/>
            </geometry>
        </collision>
    </link>
    <link name="${prefix}link_1">
        <visual>
            <geometry>
                <mesh filename="package://motoman_gp225_support/meshes/gp225/visual/link_1.dae"/>
            </geometry>
            <xacro:material_yaskawa_blue/>
        </visual>
        <collision>
            <geometry>
                <mesh filename="package://motoman_gp225_support/meshes/gp225/collision/link_1.stl"/>
            </geometry>
        </collision>
    </link>
    <link name="${prefix}link_2">
        <visual>
            <geometry>
                <mesh filename="package://motoman_gp225_support/meshes/gp225/visual/link_2.dae"/>
            </geometry>
            <xacro:material_yaskawa_blue/>
        </visual>
        <collision>
            <geometry>
                <mesh filename="package://motoman_gp225_support/meshes/gp225/collision/link_2.stl"/>
            </geometry>
        </collision>
    </link>
    <link name="${prefix}link_3">
        <visual>
            <geometry>
                <mesh filename="package://motoman_gp225_support/meshes/gp225/visual/link_3.dae"/>
            </geometry>
            <xacro:material_yaskawa_blue/>
        </visual>
        <collision>
            <geometry>
                <mesh filename="package://motoman_gp225_support/meshes/gp225/collision/link_3.stl"/>
            </geometry>
        </collision>
    </link>
    <link name="${prefix}link_4">
        <visual>
            <geometry>
                <mesh filename="package://motoman_gp225_support/meshes/gp225/visual/link_4.dae"/>
            </geometry>
            <xacro:material_yaskawa_blue/>
        </visual>
        <collision>
            <geometry>
                <mesh filename="package://motoman_gp225_support/meshes/gp225/collision/link_4.stl"/>
            </geometry>
        </collision>
    </link>
    <link name="${prefix}link_5">
        <visual>
            <geometry>
                <mesh filename="package://motoman_gp225_support/meshes/gp225/visual/link_5.dae"/>
            </geometry>
            <xacro:material_yaskawa_blue/>
        </visual>
        <collision>
            <geometry>
                <mesh filename="package://motoman_gp225_support/meshes/gp225/collision/link_5.stl"/>
            </geometry>
        </collision>
    </link>
    <link name="${prefix}link_6">
        <visual>
            <geometry>
                <mesh filename="package://motoman_gp225_support/meshes/gp225/visual/link_6.dae"/>
            </geometry>
            <xacro:material_yaskawa_blue/>
        </visual>
        <collision>
            <geometry>
                <mesh filename="package://motoman_gp225_support/meshes/gp225/collision/link_6.stl"/>
            </geometry>
        </collision>
    </link>
    <!-- end of link list -->

    <!-- joint list -->
    <joint name="${prefix}joint_1" type="revolute">
        <parent link="${prefix}base_link"/>
        <child link="${prefix}link_1"/>
        <origin xyz="0 0 0.650" rpy="0 0 0" />
        <axis xyz="0 0 1" />
        <limit lower="${radians(-180)}" upper="${radians(180)}" effort="7845.3" velocity="${radians(120)}"/>
    </joint>
    <joint name="${prefix}joint_2" type="revolute">
        <parent link="${prefix}link_1"/>
        <child link="${prefix}link_2"/>
        <origin xyz="0.325 0 0" rpy="0 0 0" />
        <axis xyz="0 1 0" />
        <limit lower="${radians(-60)}" upper="${radians(76)}" effort="11522.8" velocity="${radians(97)}"/>
    </joint>
    <joint name="${prefix}joint_3" type="revolute">
        <parent link="${prefix}link_2"/>
        <child link="${prefix}link_3"/>
        <origin xyz="0 0 1.150" rpy="0 0 0" />
        <axis xyz="0 -1 0" />
        <limit lower="${radians(-86)}" upper="${radians(90)}" effort="12258.3" velocity="${radians(115)}"/>
    </joint>
    <joint name="${prefix}joint_4" type="revolute">
        <parent link="${prefix}link_3"/>
        <child link="${prefix}link_4"/>
        <origin xyz="1.225 0 0.300" rpy="0 0 0" />
        <axis xyz="-1 0 0" />
        <limit lower="${radians(-360)}" upper="${radians(360)}" effort="3064.5" velocity="${radians(145)}"/>
    </joint>
    <joint name="${prefix}joint_5" type="revolute">
        <parent link="${prefix}link_4"/>
        <child link="${prefix}link_5"/>
        <origin xyz="0 0 0" rpy="0 0 0" />
        <axis xyz="0 -1 0" />
        <limit lower="${radians(-125)}" upper="${radians(125)}" effort="3432.3" velocity="${radians(145)}"/>
    </joint>
    <joint name="${prefix}joint_6" type="revolute">
        <parent link="${prefix}link_5"/>
        <child link="${prefix}link_6"/>
        <origin xyz="0 0 0" rpy="0 0 0" />
        <axis xyz="-1 0 0" />
        <limit lower="${radians(-360)}" upper="${radians(360)}" effort="2250.6" velocity="${radians(220)}"/>
    </joint>

    <!-- balancer -->
    <link name="${prefix}balancer_1">
        <visual>
            <geometry>
                <mesh filename="package://motoman_gp225_support/meshes/gp225/visual/balancer_1.dae"/>
            </geometry>
            <xacro:material_yaskawa_blue/>
        </visual>
        <collision>
            <geometry>
                <mesh filename="package://motoman_gp225_support/meshes/gp225/collision/balancer_1.stl"/>
            </geometry>
        </collision>
    </link>
    <link name="${prefix}balancer_2">
        <visual>
            <geometry>
                <mesh filename="package://motoman_gp225_support/meshes/gp225/visual/balancer_2.dae"/>
            </geometry>
            <xacro:material_yaskawa_blue/>
        </visual>
        <!-- collision mesh is fully contained within link_2 -->
    </link>
    <joint name="${prefix}balancer_joint_1" type="revolute">
        <parent link="${prefix}link_1"/>
        <child link="${prefix}balancer_1"/>
        <origin xyz="0.3989 0 -0.2576" rpy="0 0 0" />
        <axis xyz="0 1 0" />
        <limit lower="${radians(-60)}" upper="${radians(76)}" effort="11522.8" velocity="${radians(97)}"/>
        <mimic joint="${prefix}joint_2" multiplier="0.7634"/>
    </joint>
    <joint name="${prefix}balancer_joint_2" type="revolute">
        <parent link="${prefix}link_2"/>
        <child link="${prefix}balancer_2"/>
        <origin xyz="0 0 0.720" rpy="0 0 0" />
        <axis xyz="0 1 0" />
        <limit lower="${radians(-60)}" upper="${radians(76)}" effort="11522.8" velocity="${radians(97)}"/>
        <mimic joint="${prefix}joint_2" multiplier="-0.2366"/>
    </joint>
    <!-- end of joint list -->

    <!-- ROS base_link to Robot Manufacturer World Coordinates transform -->
    <link name="${prefix}base" />
    <joint name="${prefix}base_link-base" type="fixed">
        <origin xyz="0 0 0.650" rpy="0 0 0"/>
        <parent link="${prefix}base_link"/>
        <child link="${prefix}base"/>
    </joint>

    <!-- ROS-Industrial 'flange' frame: attachment point for EEF models -->
    <link name="${prefix}flange" />
    <joint name="${prefix}joint_6-flange" type="fixed">
        <origin xyz="0.250 0 0" rpy="0 0 0" />
        <parent link="${prefix}link_6" />
        <child link="${prefix}flange" />
    </joint>

    <!-- ROS-Industrial 'tool0' frame: all-zeros tool frame -->
    <link name="${prefix}tool0" />
    <joint name="${prefix}flange-tool0" type="fixed">
        <origin xyz="0 0 0" rpy="${radians(180)} ${radians(-90)} 0" />
        <parent link="${prefix}flange" />
        <child link="${prefix}tool0" />
    </joint>
    
</xacro:macro>
</robot>
