<?xml version="1.0" ?>
<robot xmlns:xacro="http://ros.org/wiki/xacro">

    <xacro:property name="deg" value="0.017453293" /> <!--degrees to radians-->
    
    <xacro:macro name="motoman_sia5d" params="prefix">

  <xacro:include filename="$(find motoman_resources)/urdf/common_materials.xacro"/>

  <link name="${prefix}base_link">
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <geometry>
        <mesh filename="package://motoman_sia5d_support/meshes/sia5d/visual/base_link.dae" />
      </geometry>
    </visual>
    <collision>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <geometry>
        <mesh filename="package://motoman_sia5d_support/meshes/sia5d/collision/base_link.dae" />
      </geometry>
    </collision>
  </link>
  
  <link name="${prefix}link_1">
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <geometry>
        <mesh filename="package://motoman_sia5d_support/meshes/sia5d/visual/link_1.dae" />
      </geometry>
    </visual>
    <collision>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <geometry>
        <mesh filename="package://motoman_sia5d_support/meshes/sia5d/collision/link_1.dae" />
      </geometry>
    </collision>
  </link>
  
  <joint name="${prefix}joint_1" type="revolute">
    <origin xyz="0 0 0.31" rpy="0 0 0" /> <!--r=${90*deg}-->
    <parent link="${prefix}base_link" />
    <child link="${prefix}link_1" />
    <axis xyz="0 0 1" />
    <limit lower="${-180*deg}" upper="${180*deg}" effort="0" velocity="${200*deg}" />
  </joint>

  <link name="${prefix}link_2">
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <geometry>
        <mesh filename="package://motoman_sia5d_support/meshes/sia5d/visual/link_2.dae" />
      </geometry>
    </visual>
    <collision>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <geometry>
        <mesh filename="package://motoman_sia5d_support/meshes/sia5d/collision/link_2.dae" />
      </geometry>
    </collision>
  </link>
  
  <joint name="${prefix}joint_2" type="revolute">
    <origin xyz="0 0 0" rpy="0 0 0" />
    <parent link="${prefix}link_1" />
    <child link="${prefix}link_2" />
    <axis xyz="0 1 0" />
    <limit lower="${-110*deg}" upper="${110*deg}" effort="0" velocity="${200*deg}" />
  </joint>

  <link name="${prefix}link_3">
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <geometry>
        <mesh filename="package://motoman_sia5d_support/meshes/sia5d/visual/link_3.dae" />
      </geometry>
    </visual>
    <collision>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <geometry>
        <mesh filename="package://motoman_sia5d_support/meshes/sia5d/collision/link_3.dae" />
      </geometry>
    </collision>
  </link>
  
  <joint name="${prefix}joint_3" type="revolute">
    <origin xyz="0 0 0.27" rpy="0 0 0" />
    <parent link="${prefix}link_2" />
    <child link="${prefix}link_3" />
    <axis xyz="0 0 1" />
    <limit lower="${-170*deg}" upper="${170*deg}" effort="0" velocity="${200*deg}" />
  </joint>
  
  <link name="${prefix}link_4">
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <geometry>
        <mesh filename="package://motoman_sia5d_support/meshes/sia5d/visual/link_4.dae" />
      </geometry>
    </visual>
    <collision>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <geometry>
        <mesh filename="package://motoman_sia5d_support/meshes/sia5d/collision/link_4.dae" />
      </geometry>
    </collision>
  </link>
  
  <joint name="${prefix}joint_4" type="revolute">
    <origin xyz="0.085 0 0" rpy="0 0 0" />
    <parent link="${prefix}link_3" />
    <child link="${prefix}link_4" />
    <axis xyz="0 -1 0" />
    <limit lower="${-90*deg}" upper="${115*deg}" effort="0"  velocity="${200*deg}" />
  </joint>

  <link name="${prefix}link_5">
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <geometry>
        <mesh filename="package://motoman_sia5d_support/meshes/sia5d/visual/link_5.dae" />
      </geometry>
    </visual>
    <collision>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <geometry>
        <mesh filename="package://motoman_sia5d_support/meshes/sia5d/collision/link_5.dae" />
      </geometry>
    </collision>
  </link>
  
  <joint name="${prefix}joint_5" type="revolute">
    <origin xyz="0.27 0 0.06" rpy="0 0 0" />
    <parent link="${prefix}link_4" />
    <child link="${prefix}link_5" />
    <axis xyz="-1 0 0" />
    <limit lower="${-90*deg}" upper="${90*deg}" effort="0" velocity="${200*deg}" />
  </joint>

  <link name="${prefix}link_6">
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <geometry>
        <mesh filename="package://motoman_sia5d_support/meshes/sia5d/visual/link_6.dae" />
      </geometry>
    </visual>
    <collision>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <geometry>
        <mesh filename="package://motoman_sia5d_support/meshes/sia5d/collision/link_6.dae" />
      </geometry>
    </collision>
  </link>

  <joint name="${prefix}joint_6" type="revolute">
    <origin xyz="0 0 0" rpy="0 0 0" />
    <parent link="${prefix}link_5" />
    <child link="${prefix}link_6" />
    <axis xyz="0 -1 0" />
    <limit lower="${-110*deg}" upper="${110*deg}" effort="0" velocity="${230*deg}" />
  </joint>

  <link name="${prefix}link_7">
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <geometry>
        <mesh filename="package://motoman_sia5d_support/meshes/sia5d/visual/link_7.dae" />
      </geometry>
    </visual>
    <collision>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <geometry>
        <mesh filename="package://motoman_sia5d_support/meshes/sia5d/collision/link_7.dae" />
      </geometry>
    </collision>
  </link>
  
  <joint name="${prefix}joint_7" type="revolute">
    <origin xyz="0.134 0 0" rpy="0 0 0" />
    <parent link="${prefix}link_6" />
    <child link="${prefix}link_7" />
    <axis xyz="-1 0 0" />
    <limit lower="${-90*deg}" upper="${90*deg}" effort="0" velocity="${350*deg}" />
  </joint>
  <!-- end of joint list -->
  
  <!-- ROS base_link (via base_link) to Motoman Robot (not Base) Frame transform -->
    <link name="${prefix}base" />
    <joint name="${prefix}base_link-base" type="fixed">
      <origin xyz="0 0 0.310" rpy="0 0 0"/>
      <parent link="${prefix}base_link"/>
      <child link="${prefix}base"/>
    </joint>
    </xacro:macro>

  <!-- ROS-Industrial 'flange' frame: attachment point for EEF models -->
  <link name="${prefix}flange"/>
  <joint name="${prefix}joint_7-flange" type="fixed">
    <origin xyz="0.011 0 0" rpy="0 0 0"/>
    <parent link="${prefix}link_7"/>
    <child link="${prefix}flange"/>
  </joint>

  <!-- ROS-Industrial 'tool0' frame: all-zeros tool frame -->
  <link name="${prefix}tool0"/>
  <joint name="${prefix}flange-tool0" type="fixed">
    <origin xyz="0 0 0 " rpy="${180*deg} ${-90*deg} 0"/>
    <parent link="${prefix}flange"/>
    <child link="${prefix}tool0"/>
  </joint>

</robot>

