<launch>
  <arg name="req_arg" value="default"/>
  <arg name="dx200" value="dx200"/>
  
  <group ns="load_motopos_mh1655">
    <include file="$(find motoman_motopos_mh1655_support)/launch/load_motopos_mh1655.launch"/>
  </group>

  <group ns="test_motopos_mh1655">
    <include file="$(find motoman_motopos_mh1655_support)/launch/test_motopos_mh1655.launch"/>
  </group>

  <group ns="robot_interface_streaming_motopos_mh1655">
    <group ns="dx200" >
      <include file="$(find motoman_motopos_mh1655_support)/launch/robot_interface_streaming_motopos_mh1655.launch">
        <arg name="robot_ip"   value="$(arg req_arg)" />
        <arg name="controller" value="$(arg dx200)"/>
      </include>
    </group>
  </group>

  <group ns="robot_state_visualize_motopos_mh1655">
    <group ns="dx200" >
      <include file="$(find motoman_motopos_mh1655_support)/launch/robot_state_visualize_motopos_mh1655.launch">
        <arg name="robot_ip"   value="$(arg req_arg)" />
        <arg name="controller" value="$(arg dx200)"/>
      </include>
    </group>
  </group>

</launch>
