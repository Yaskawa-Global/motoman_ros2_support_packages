<?xml version="1.0" ?>
<robot xmlns:xacro="http://ros.org/wiki/xacro">
  <xacro:macro name="motoman_gp70l" params="prefix">
    <xacro:include filename="$(find motoman_resources)/urdf/common_materials.xacro"/>

    <!-- link list -->
    <link name="${prefix}base_link">
      <visual>
        <geometry>
          <mesh filename="package://motoman_gp70l_support/meshes/gp70l/visual/base_link.dae"/>
        </geometry>
        <xacro:material_yaskawa_blue/>
      </visual>
      <collision>
        <geometry>
          <mesh filename="package://motoman_gp70l_support/meshes/gp70l/collision/base_link.stl"/>
        </geometry>
      </collision>
    </link>
    <link name="${prefix}link_1">
      <visual>
        <geometry>
          <mesh filename="package://motoman_gp70l_support/meshes/gp70l/visual/link_1.dae"/>
        </geometry>
        <xacro:material_yaskawa_blue/>
      </visual>
      <collision>
        <geometry>
          <mesh filename="package://motoman_gp70l_support/meshes/gp70l/collision/link_1.stl"/>
        </geometry>
      </collision>
    </link>
    <link name="${prefix}link_2">
      <visual>
        <geometry>
          <mesh filename="package://motoman_gp70l_support/meshes/gp70l/visual/link_2.dae"/>
        </geometry>
        <xacro:material_yaskawa_blue/>
      </visual>
      <collision>
        <geometry>
          <mesh filename="package://motoman_gp70l_support/meshes/gp70l/collision/link_2.stl"/>
        </geometry>
      </collision>
    </link>
    <link name="${prefix}link_3">
      <visual>
        <geometry>
          <mesh filename="package://motoman_gp70l_support/meshes/gp70l/visual/link_3.dae"/>
        </geometry>
        <xacro:material_yaskawa_blue/>
      </visual>
      <collision>
        <geometry>
          <mesh filename="package://motoman_gp70l_support/meshes/gp70l/collision/link_3.stl"/>
        </geometry>
      </collision>
    </link>
    <link name="${prefix}link_4">
      <visual>
        <geometry>
          <mesh filename="package://motoman_gp70l_support/meshes/gp70l/visual/link_4.dae"/>
        </geometry>
        <xacro:material_yaskawa_blue/>
      </visual>
      <collision>
        <geometry>
          <mesh filename="package://motoman_gp70l_support/meshes/gp70l/collision/link_4.stl"/>
        </geometry>
      </collision>
    </link>
    <link name="${prefix}link_5">
      <visual>
        <geometry>
          <mesh filename="package://motoman_gp70l_support/meshes/gp70l/visual/link_5.dae"/>
        </geometry>
        <xacro:material_yaskawa_blue/>
      </visual>
      <collision>
        <geometry>
          <mesh filename="package://motoman_gp70l_support/meshes/gp70l/collision/link_5.stl"/>
        </geometry>
      </collision>
    </link>
    <link name="${prefix}link_6">
      <visual>
        <geometry>
          <mesh filename="package://motoman_gp70l_support/meshes/gp70l/visual/link_6.dae"/>
        </geometry>
        <xacro:material_yaskawa_silver/>
      </visual>
      <collision>
        <geometry>
          <mesh filename="package://motoman_gp70l_support/meshes/gp70l/collision/link_6.stl"/>
        </geometry>
      </collision>
    </link>
    <!-- end of link list -->

    <!-- joint list -->
    <joint name="${prefix}joint_1" type="revolute">
      <parent link="${prefix}base_link"/>
      <child link="${prefix}link_1"/>
      <origin xyz="0 0 0.540" rpy="0 0 0" />
      <axis xyz="0 0 1" />
      <limit lower="${radians(-180)}" upper="${radians(180)}" effort="3983.9" velocity="${radians(180.0)}"/>
    </joint>
    <joint name="${prefix}joint_2" type="revolute">
      <parent link="${prefix}link_1"/>
      <child link="${prefix}link_2"/>
      <origin xyz="0.320 0 0" rpy="0 0 0" />
      <axis xyz="0 1 0" />
      <limit lower="${radians(-90)}" upper="${radians(155)}" effort="10174.4" velocity="${radians(123.0)}"/>
    </joint>
    <joint name="${prefix}joint_3" type="revolute">
      <parent link="${prefix}link_2"/>
      <child link="${prefix}link_3"/>
      <origin xyz="0 0 1.165" rpy="0 0 0" />
      <axis xyz="0 -1 0" />
      <limit lower="${radians(-80)}" upper="${radians(90)}" effort="5148.4" velocity="${radians(160.0)}"/>
    </joint>
    <joint name="${prefix}joint_4" type="revolute">
      <parent link="${prefix}link_3"/>
      <child link="${prefix}link_4"/>
      <origin xyz="1.225 0 0.235" rpy="0 0 0" />
      <axis xyz="-1 0 0" />
      <limit lower="${radians(-360)}" upper="${radians(360)}" effort="823.7" velocity="${radians(230)}"/>
    </joint>
    <joint name="${prefix}joint_5" type="revolute">
      <parent link="${prefix}link_4"/>
      <child link="${prefix}link_5"/>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <axis xyz="0 -1 0" />
      <limit lower="${radians(-125)}" upper="${radians(125)}" effort="794.3" velocity="${radians(230)}"/>
    </joint>
    <joint name="${prefix}joint_6" type="revolute">
      <parent link="${prefix}link_5"/>
      <child link="${prefix}link_6"/>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <axis xyz="-1 0 0" />
      <limit lower="${radians(-360)}" upper="${radians(360)}" effort="333.4" velocity="${radians(350)}"/>
    </joint>
    <!-- end of joint list -->

    <!-- ROS base_link to Robot Manufacturer World Coordinates transform -->
    <link name="${prefix}base" />
    <joint name="${prefix}base_link-base" type="fixed">
      <origin xyz="0 0 0.540" rpy="0 0 0"/>
      <parent link="${prefix}base_link"/>
      <child link="${prefix}base"/>
    </joint>

    <!-- ROS-Industrial 'flange' frame: attachment point for EEF models -->
    <link name="${prefix}flange"/>
    <joint name="${prefix}joint_6-flange" type="fixed">
      <origin xyz="0.175 0 0" rpy="0 0 0"/>
      <parent link="${prefix}link_6"/>
      <child link="${prefix}flange"/>
    </joint>

    <!-- ROS-Industrial 'tool0' frame: all-zeros tool frame -->
    <link name="${prefix}tool0"/>
    <joint name="${prefix}flange-tool0" type="fixed">
      <origin xyz="0 0 0" rpy="${radians(180)} ${radians(-90)} 0"/>
      <parent link="${prefix}flange"/>
      <child link="${prefix}tool0"/>
    </joint>

  </xacro:macro>
</robot>
