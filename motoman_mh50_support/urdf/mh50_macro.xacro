<?xml version="1.0" ?>
<robot xmlns:xacro="http://ros.org/wiki/xacro">
<xacro:macro name="motoman_mh50" params="prefix">
    <xacro:include filename="$(find motoman_resources)/urdf/common_materials.xacro"/>

    <!-- link list -->
    <link name="${prefix}base_link">
        <visual>
            <geometry>
                <mesh filename="package://motoman_mh50_support/meshes/visual/base_link.stl"/>
            </geometry>
            <xacro:material_yaskawa_blue/>
        </visual>
        <collision>
            <geometry>
                <mesh filename="package://motoman_mh50_support/meshes/collision/base_link.stl"/>
            </geometry>
        </collision>
    </link>
    <link name="${prefix}link_1">
        <visual>
            <geometry>
                <mesh filename="package://motoman_mh50_support/meshes/visual/link_1.stl"/>
            </geometry>
            <xacro:material_yaskawa_blue/>
        </visual>
        <collision>
            <geometry>
                <mesh filename="package://motoman_mh50_support/meshes/collision/link_1.stl"/>
            </geometry>
        </collision>
    </link>
    <link name="${prefix}link_2">
        <visual>
            <geometry>
                <mesh filename="package://motoman_mh50_support/meshes/visual/link_2.stl"/>
            </geometry>
            <xacro:material_yaskawa_blue/>
        </visual>
        <collision>
            <geometry>
                <mesh filename="package://motoman_mh50_support/meshes/collision/link_2.stl"/>
            </geometry>
        </collision>
    </link>
    <link name="${prefix}link_3">
        <visual>
            <geometry>
                <mesh filename="package://motoman_mh50_support/meshes/visual/link_3.stl"/>
            </geometry>
            <xacro:material_yaskawa_blue/>
        </visual>
        <collision>
            <geometry>
                <mesh filename="package://motoman_mh50_support/meshes/collision/link_3.stl"/>
            </geometry>
        </collision>
    </link>
    <link name="${prefix}link_4">
        <visual>
            <geometry>
                <mesh filename="package://motoman_mh50_support/meshes/visual/link_4.stl"/>
            </geometry>
            <xacro:material_yaskawa_blue/>
        </visual>
        <collision>
            <geometry>
                <mesh filename="package://motoman_mh50_support/meshes/collision/link_4.stl"/>
            </geometry>
        </collision>
    </link>
    <link name="${prefix}link_5">
        <visual>
            <geometry>
                <mesh filename="package://motoman_mh50_support/meshes/visual/link_5.stl"/>
            </geometry>
            <xacro:material_yaskawa_blue/>
        </visual>
        <collision>
            <geometry>
                <mesh filename="package://motoman_mh50_support/meshes/collision/link_5.stl"/>
            </geometry>
        </collision>
    </link>
    <link name="${prefix}link_6">
        <visual>
            <geometry>
                <mesh filename="package://motoman_mh50_support/meshes/visual/link_6.stl"/>
            </geometry>
            <xacro:material_yaskawa_blue/>
        </visual>
        <collision>
            <geometry>
                <mesh filename="package://motoman_mh50_support/meshes/collision/link_6.stl"/>
            </geometry>
        </collision>
    </link>
    <!-- end of link list -->

    <!-- joint list -->
    <joint name="${prefix}joint_1" type="revolute">
        <parent link="${prefix}base_link"/>
        <child link="${prefix}link_1"/>
        <origin xyz="0 0 0.540" rpy="0 0 0" />
        <axis xyz="0 0 1" />
        <limit lower="-3.14159" upper="3.14159" effort="4903.32" velocity="3.141"/>
    </joint>
    <joint name="${prefix}joint_2" type="revolute">
        <parent link="${prefix}link_1"/>
        <child link="${prefix}link_2"/>
        <origin xyz="0.145 0 0" rpy="0 0 0" />
        <axis xyz="0 1 0" />
        <limit lower="-1.57079" upper="2.35619" effort="6276.26" velocity="3.103"/>
    </joint>
    <joint name="${prefix}joint_3" type="revolute">
        <parent link="${prefix}link_2"/>
        <child link="${prefix}link_3"/>
        <origin xyz="0 0 0.870" rpy="0 0 0" />
        <axis xyz="0 -1 0" />
        <limit lower="-1.39626" upper="3.59538" effort="2745.86" velocity="3.106"/>
    </joint>
    <joint name="${prefix}joint_4" type="revolute">
        <parent link="${prefix}link_3"/>
        <child link="${prefix}link_4"/>
        <origin xyz="1.025 0 0.210" rpy="0 0 0"/>
        <axis xyz="-1 0 0" />
        <limit lower="-6.28319" upper="6.28319" effort="823.76" velocity="4.363"/>
    </joint>
    <joint name="${prefix}joint_5" type="revolute">
        <parent link="${prefix}link_4"/>
        <child link="${prefix}link_5"/>
        <origin xyz="0 0 0" rpy="0 0 0" />
        <axis xyz="0 -1 0" />
        <limit lower="-2.18167" upper="2.18167" effort="706.08" velocity="4.363"/>
    </joint>
    <joint name="${prefix}joint_6" type="revolute">
        <parent link="${prefix}link_5"/>
        <child link="${prefix}link_6"/>
        <origin xyz="0 0 0" rpy="0 0 0" />
        <axis xyz="-1 0 0" />
        <limit lower="-6.2831" upper="6.2831" effort="333.43" velocity="6.283"/>
    </joint>
    <!-- end of joint list -->

    <!-- ROS base_link to Robot Manufacturer World Coordinates transform -->
    <link name="${prefix}base" />
    <joint name="${prefix}base_link-base" type="fixed">
        <origin xyz="0 0 0.540" rpy="0 0 0"/>
        <parent link="${prefix}base_link"/>
        <child link="${prefix}base"/>
    </joint>
    
    <!-- ROS-Industrial 'flange' frame: attachment point for EEF models -->
    <link name="${prefix}flange"/>
    <joint name="${prefix}joint_6-flange" type="fixed">
      <origin xyz="0.175 0 0" rpy="0 0 0"/>
      <parent link="${prefix}link_6"/>
      <child link="${prefix}flange"/>
    </joint>

    <!-- ROS-Industrial 'tool0' frame: all-zeros tool frame -->
    <link name="${prefix}tool0"/>
    <joint name="${prefix}flange-tool0" type="fixed">
      <origin xyz="0 0 0" rpy="3.1415926 -1.570796 0"/>
      <parent link="${prefix}flange"/>
      <child link="${prefix}tool0"/>
    </joint>

    </xacro:macro>
</robot>

