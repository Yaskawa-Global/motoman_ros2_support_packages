<?xml version="1.0" ?>
<robot xmlns:xacro="http://ros.org/wiki/xacro">
  <xacro:macro name="motoman_sia30d" params="prefix">
    <xacro:include filename="$(find motoman_resources)/urdf/common_materials.xacro"/>

    <!-- link list -->
    <link name="${prefix}base_link">
      <visual>
        <geometry>
          <mesh filename="package://motoman_sia30d_support/meshes/visual/base_link.stl"/>
        </geometry>
        <xacro:material_yaskawa_blue/>
      </visual>
      <collision>
        <geometry>
          <mesh filename="package://motoman_sia30d_support/meshes/collision/base_link.stl"/>
        </geometry>
      </collision>
    </link>
    <link name="${prefix}link_1_s">
      <visual>
        <geometry>
          <mesh filename="package://motoman_sia30d_support/meshes/visual/link_1.stl"/>
        </geometry>
        <xacro:material_yaskawa_silver/>
      </visual>
      <collision>
        <geometry>
          <mesh filename="package://motoman_sia30d_support/meshes/collision/link_1.stl"/>
        </geometry>
      </collision>
    </link>
    <link name="${prefix}link_2_l">
      <visual>
        <geometry>
          <mesh filename="package://motoman_sia30d_support/meshes/visual/link_2.stl"/>
        </geometry>
        <xacro:material_yaskawa_blue/>
      </visual>
      <collision>
        <geometry>
          <mesh filename="package://motoman_sia30d_support/meshes/collision/link_2.stl"/>
        </geometry>
      </collision>
    </link>
    <link name="${prefix}link_3_e">
      <visual>
        <geometry>
          <mesh filename="package://motoman_sia30d_support/meshes/visual/link_3.stl"/>
        </geometry>
        <xacro:material_yaskawa_silver/>
      </visual>
      <collision>
        <geometry>
          <mesh filename="package://motoman_sia30d_support/meshes/collision/link_3.stl"/>
        </geometry>
      </collision>
    </link>
    <link name="${prefix}link_4_u">
      <visual>
        <geometry>
          <mesh filename="package://motoman_sia30d_support/meshes/visual/link_4.stl"/>
        </geometry>
        <xacro:material_yaskawa_blue/>
      </visual>
      <collision>
        <geometry>
          <mesh filename="package://motoman_sia30d_support/meshes/collision/link_4.stl"/>
        </geometry>
      </collision>
    </link>
    <link name="${prefix}link_5_r">
      <visual>
        <geometry>
          <mesh filename="package://motoman_sia30d_support/meshes/visual/link_5.stl"/>
        </geometry>
        <xacro:material_yaskawa_silver/>
      </visual>
      <collision>
        <geometry>
          <mesh filename="package://motoman_sia30d_support/meshes/collision/link_5.stl"/>
        </geometry>
      </collision>
    </link>
    <link name="${prefix}link_6_b">
      <visual>
        <geometry>
          <mesh filename="package://motoman_sia30d_support/meshes/visual/link_6.stl"/>
        </geometry>
        <xacro:material_yaskawa_blue/>
      </visual>
      <collision>
        <geometry>
          <mesh filename="package://motoman_sia30d_support/meshes/collision/link_6.stl"/>
        </geometry>
      </collision>
    </link>
    <link name="${prefix}link_7_t">
      <visual>
        <geometry>
          <mesh filename="package://motoman_sia30d_support/meshes/visual/link_7.stl"/>
        </geometry>
        <xacro:material_yaskawa_silver/>
      </visual>
      <collision>
        <geometry>
          <mesh filename="package://motoman_sia30d_support/meshes/collision/link_7.stl"/>
        </geometry>
      </collision>
    </link>
    <!-- end of link list -->

    <!-- joint list -->
    <joint name="${prefix}joint_1_s" type="revolute">
      <parent link="${prefix}base_link"/>
      <child link="${prefix}link_1_s"/>
      <origin xyz="0 0 0.598" rpy="0 0 0" />
      <axis xyz="0 0 1" />
      <limit lower="${radians(-180)}" upper="${radians(180)}" effort="1911" velocity="${radians(130)}"/>
    </joint>
    <joint name="${prefix}joint_2_l" type="revolute">
      <parent link="${prefix}link_1_s"/>
      <child link="${prefix}link_2_l"/>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <axis xyz="0 1 0" />
      <limit lower="${radians(-125)}" upper="${radians(125)}" effort="2388" velocity="${radians(130)}"/>
    </joint>
    <joint name="${prefix}joint_3_e" type="revolute">
      <parent link="${prefix}link_2_l"/>
      <child link="${prefix}link_3_e"/>
      <origin xyz="0 0 0.875" rpy="0 0 0" />
      <axis xyz="0 0 1" />
      <limit lower="${radians(-170)}" upper="${radians(170)}" effort="674" velocity="${radians(130)}"/>
    </joint>
    <joint name="${prefix}joint_4_u" type="revolute">
      <parent link="${prefix}link_3_e"/>
      <child link="${prefix}link_4_u"/>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <axis xyz="0 -1 0" />
      <limit lower="${radians(-110)}" upper="${radians(110)}" effort="674" velocity="${radians(130)}"/>
    </joint>
    <joint name="${prefix}joint_5_r" type="revolute">
      <parent link="${prefix}link_4_u"/>
      <child link="${prefix}link_5_r"/>
      <origin xyz="0 0 0.610" rpy="0 0 0" />
      <axis xyz="0 0 -1" />
      <limit lower="${radians(-170)}" upper="${radians(170)}" effort="191" velocity="${radians(170)}"/>
    </joint>    
    <joint name="${prefix}joint_6_b" type="revolute">
      <parent link="${prefix}link_5_r"/>
      <child link="${prefix}link_6_b"/>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <axis xyz="0 -1 0" />
      <limit lower="${radians(-110)}" upper="${radians(110)}" effort="191" velocity="${radians(170)}"/>
    </joint>
    <joint name="${prefix}joint_7_t" type="revolute">
      <parent link="${prefix}link_6_b"/>
      <child link="${prefix}link_7_t"/>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <axis xyz="0 0 -1" />
      <limit lower="${radians(-180)}" upper="${radians(180)}" effort="77" velocity="${radians(200)}"/>
    </joint>
    <!-- end of joint list -->

    <!-- ROS base_link to Robot Manufacturer World Coordinates transform -->
    <link name="${prefix}base" />
    <joint name="${prefix}base_link-base" type="fixed">
      <origin xyz="0 0 0.598" rpy="0 0 0"/>
      <parent link="${prefix}base_link"/>
      <child link="${prefix}base"/>
    </joint>

    <!-- ROS-Industrial 'flange' frame: attachment point for EEF models -->
    <link name="${prefix}flange"/>
    <joint name="${prefix}joint_7_t-flange" type="fixed">
        <origin xyz="0 0 0.217" rpy="0 ${radians(-90)} 0"/>
        <parent link="${prefix}link_7_t"/>
        <child link="${prefix}flange"/>
    </joint>

    <!-- ROS-Industrial 'tool0' frame: all-zeros tool frame -->
    <link name="${prefix}tool0"/>
    <joint name="${prefix}flange-tool0" type="fixed">
        <origin xyz="0 0 0 " rpy="0 ${radians(-90)} ${radians(180)}"/>
        <parent link="${prefix}flange"/>
        <child link="${prefix}tool0"/>
    </joint>
  </xacro:macro>
</robot>
