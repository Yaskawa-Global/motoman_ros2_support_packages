<?xml version="1.0" ?>
<robot xmlns:xacro="http://ros.org/wiki/xacro">
  <xacro:macro name="motoman_mh5" params="prefix">
    <xacro:include filename="$(find motoman_resources)/urdf/common_materials.xacro"/>

    <!-- link list -->
    <link name="${prefix}base_link">
      <visual>
        <origin xyz="0 0 0" rpy="0 0 0" />
        <geometry>
          <mesh filename="package://motoman_mh5_support/meshes/mh5/visual/base_link.stl" />
        </geometry>
        <xacro:material_yaskawa_white/>
      </visual>
      <collision>
        <origin xyz="0 0 0" rpy="0 0 0" />
        <geometry>
          <mesh filename="package://motoman_mh5_support/meshes/mh5/collision/base_link.stl" />
        </geometry>
      </collision>
    </link>
    <link name="${prefix}link_1">
      <visual>
        <origin xyz="0 0 0" rpy="0 0 0" />
        <geometry>
          <mesh filename="package://motoman_mh5_support/meshes/mh5/visual/link_1.stl"/>
        </geometry>
        <xacro:material_yaskawa_white/>
      </visual>
      <collision>
        <origin xyz="0 0 0" rpy="0 0 0" />
        <geometry>
          <mesh filename="package://motoman_mh5_support/meshes/mh5/collision/link_1.stl" />
        </geometry>
      </collision>
    </link>
    <link name="${prefix}link_2">
      <visual>
        <origin xyz="0 0 0" rpy="0 0 0" />
        <geometry>
          <mesh filename="package://motoman_mh5_support/meshes/mh5/visual/link_2.stl"/>
        </geometry>
        <xacro:material_yaskawa_white/>
      </visual>
      <collision>
        <origin xyz="0 0 0" rpy="0 0 0" />
        <geometry>
          <mesh filename="package://motoman_mh5_support/meshes/mh5/collision/link_2.stl" />
        </geometry>
      </collision>
    </link>
    <link name="${prefix}link_3">
      <visual>
        <origin xyz="0 0 0" rpy="0 0 0" />
        <geometry>
          <mesh filename="package://motoman_mh5_support/meshes/mh5/visual/link_3.stl"/>
          <!-- <mesh filename="package://motoman_mh5_support/meshes/mh5_old_variants/visual/link_3.stl"/> --><!-- Uncomment & switch with above to use alternative U-link. -->
        </geometry>
        <xacro:material_yaskawa_white/>
      </visual>
      <collision>
        <origin xyz="0 0 0" rpy="0 0 0" />
        <geometry>
          <mesh filename="package://motoman_mh5_support/meshes/mh5/collision/link_3.stl" />
          <!-- <mesh filename="package://motoman_mh5_support/meshes/mh5_old_variants/collision/link_3.stl" /> --><!-- Uncomment & switch with above to use alternative U-link. -->
        </geometry>
      </collision>
    </link>
    <link name="${prefix}link_4">
      <visual>
        <origin xyz="0 0 0" rpy="0 0 0" />
        <geometry>
          <mesh filename="package://motoman_mh5_support/meshes/mh5/visual/link_4.stl"/>
        </geometry>
        <xacro:material_yaskawa_white/>
      </visual>
      <collision>
        <origin xyz="0 0 0" rpy="0 0 0" />
        <geometry>
          <mesh filename="package://motoman_mh5_support/meshes/mh5/collision/link_4.stl" />
        </geometry>
      </collision>
    </link>
    <link name="${prefix}link_5">
      <visual>
        <origin xyz="0 0 0" rpy="0 0 0" />
        <geometry>
          <mesh filename="package://motoman_mh5_support/meshes/mh5/visual/link_5.stl"/>
        </geometry>
        <xacro:material_yaskawa_white/>
      </visual>
      <collision>
        <origin xyz="0 0 0" rpy="0 0 0" />
        <geometry>
          <mesh filename="package://motoman_mh5_support/meshes/mh5/collision/link_5.stl" />
        </geometry>
      </collision>
    </link>
    <link name="${prefix}link_6">
      <visual>
        <origin xyz="0 0 0" rpy="0 0 0" />
        <geometry>
          <mesh filename="package://motoman_mh5_support/meshes/mh5/visual/link_6.stl" />
        </geometry>
        <xacro:material_yaskawa_white/>
      </visual>
      <collision>
        <origin xyz="0 0 0" rpy="0 0 0" />
        <geometry>
          <mesh filename="package://motoman_mh5_support/meshes/mh5/collision/link_6.stl" />
        </geometry>
      </collision>
    </link>
    <!-- end of link list -->

    <!-- joint list -->
    <joint name="${prefix}joint_1" type="revolute">
      <parent link="${prefix}base_link"/>
      <child link="${prefix}link_1"/>
      <origin xyz="0 0 0.330" rpy="0 0 0"/>
      <axis xyz="0 0 1" />
      <limit lower="-${radians(170)}" upper="${radians(170)}" effort="215.6" velocity="${radians(376)}" />
    </joint>
    <joint name="${prefix}joint_2" type="revolute">
      <parent link="${prefix}link_1"/>
      <child link="${prefix}link_2"/>
      <origin xyz="0.0880 0 0.00" rpy="0 0 0"/>
      <axis xyz="0 1 0" />
      <limit lower="-${radians(65)}" upper="${radians(150)}" effort="176.4" velocity="${radians(350)}" />
    </joint>
    <joint name="${prefix}joint_3" type="revolute">
      <parent link="${prefix}link_2"/>
      <child link="${prefix}link_3"/>
      <origin xyz="0 0 0.310" rpy="0 0 0"/>
      <axis xyz="0 -1 0" />
      <limit lower="-${radians(70)}" upper="${radians(190)}" effort="76.8" velocity="${radians(400)}" />
    </joint>
    <joint name="${prefix}joint_4" type="revolute">
      <parent link="${prefix}link_3"/>
      <child link="${prefix}link_4"/>
      <origin xyz="0 0 0.040" rpy="0 0 0"/>
      <axis xyz="-1 0 0" />
      <limit lower="-${radians(190)}" upper="${radians(190)}" effort="44.7" velocity="${radians(450)}" />
    </joint>
    <joint name="${prefix}joint_5" type="revolute">
      <parent link="${prefix}link_4"/>
      <child link="${prefix}link_5"/>
      <origin xyz="0.305 0 0" rpy="0 0 0"/>
      <axis xyz="0 -1 0" />
      <limit lower="-${radians(135)}" upper="${radians(135)}" effort="18.0" velocity="${radians(450)}" />
    </joint>
    <joint name="${prefix}joint_6" type="revolute">
      <parent link="${prefix}link_5"/>
      <child link="${prefix}link_6"/>
      <origin xyz="0.080 0 0" rpy="0 0 0"/>
      <axis xyz="-1 0 0" />
      <limit lower="-${radians(360)}" upper="${radians(360)}" effort="18.0" velocity="${radians(720)}" />
    </joint>
    <!-- end of joint list -->

    <!-- ROS base_link to Robot Manufacturer (Motoman) World Coordinates transform -->
    <link name="${prefix}base" />
    <joint name="${prefix}base_link-base" type="fixed">
        <parent link="${prefix}base_link"/>
        <child link="${prefix}base"/>
        <origin xyz="0 0 0.330" rpy="0 0 0"/>
    </joint>

    <!-- ROS-Industrial 'flange' frame: attachment point for EEF models -->
    <link name="${prefix}flange" />
    <joint name="${prefix}joint_6-flange" type="fixed">
      <parent link="${prefix}link_6" />
      <child link="${prefix}flange" />
      <origin xyz="0 0 0" rpy="0 0 0" />
    </joint>

    <!-- ROS-Industrial 'tool0' frame: all-zeros tool frame -->
    <link name="${prefix}tool0"/>
    <joint name="${prefix}joint_6-tool0" type="fixed">
        <parent link="${prefix}link_6"/>
        <child link="${prefix}tool0"/>
        <origin xyz="0 0 0" rpy="${radians(180)} ${radians(-90)} 0"/>
    </joint>

  </xacro:macro>
</robot>
