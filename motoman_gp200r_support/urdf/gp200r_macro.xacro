<?xml version="1.0" ?>
<robot xmlns:xacro="http://ros.org/wiki/xacro">
<xacro:macro name="motoman_gp200r" params="prefix">

    <!-- Properties -->
    <xacro:include filename="$(find motoman_resources)/urdf/common_materials.xacro"/>

    <!-- link list -->
    <link name="${prefix}base_link">
        <visual>
            <geometry>
                <mesh filename="package://motoman_gp200r_support/meshes/gp200r/visual/base_link.stl"/>
            </geometry>
            <xacro:material_yaskawa_blue/>
        </visual>
        <collision>
            <geometry>
                <mesh filename="package://motoman_gp200r_support/meshes/gp200r/collision/base_link.stl"/>
            </geometry>
        </collision>
    </link>
    <link name="${prefix}link_1">
        <visual>
            <geometry>
                <mesh filename="package://motoman_gp200r_support/meshes/gp200r/visual/link_1.stl"/>
            </geometry>
            <xacro:material_yaskawa_blue/>
        </visual>
        <collision>
            <geometry>
                <mesh filename="package://motoman_gp200r_support/meshes/gp200r/collision/link_1.stl"/>
            </geometry>
        </collision>
    </link>
    <link name="${prefix}link_2">
        <visual>
            <geometry>
                <mesh filename="package://motoman_gp200r_support/meshes/gp200r/visual/link_2.stl"/>
            </geometry>
            <xacro:material_yaskawa_blue/>
        </visual>
        <collision>
            <geometry>
                <mesh filename="package://motoman_gp200r_support/meshes/gp200r/collision/link_2.stl"/>
            </geometry>
        </collision>
    </link>
    <link name="${prefix}link_3">
        <visual>
            <geometry>
                <mesh filename="package://motoman_gp200r_support/meshes/gp200r/visual/link_3.stl"/>
            </geometry>
            <xacro:material_yaskawa_blue/>
        </visual>
        <collision>
            <geometry>
                <mesh filename="package://motoman_gp200r_support/meshes/gp200r/collision/link_3.stl"/>
            </geometry>
        </collision>
    </link>
    <link name="${prefix}link_4">
        <visual>
            <geometry>
                <mesh filename="package://motoman_gp200r_support/meshes/gp200r/visual/link_4.stl"/>
            </geometry>
            <xacro:material_yaskawa_blue/>
        </visual>
        <collision>
            <geometry>
                <mesh filename="package://motoman_gp200r_support/meshes/gp200r/collision/link_4.stl"/>
            </geometry>
        </collision>
    </link>
    <link name="${prefix}link_5">
        <visual>
            <geometry>
                <mesh filename="package://motoman_gp200r_support/meshes/gp200r/visual/link_5.stl"/>
            </geometry>
            <xacro:material_yaskawa_blue/>
        </visual>
        <collision>
            <geometry>
                <mesh filename="package://motoman_gp200r_support/meshes/gp200r/collision/link_5.stl"/>
            </geometry>
        </collision>
    </link>
    <link name="${prefix}link_6">
        <visual>
            <geometry>
                <mesh filename="package://motoman_gp200r_support/meshes/gp200r/visual/link_6.stl"/>
            </geometry>
            <xacro:material_yaskawa_blue/>
        </visual>
        <collision>
            <geometry>
                <mesh filename="package://motoman_gp200r_support/meshes/gp200r/collision/link_6.stl"/>
            </geometry>
        </collision>
    </link>
    <!-- end of link list -->

    <!-- joint list -->
    <joint name="${prefix}joint_1" type="revolute">
        <parent link="${prefix}base_link"/>
        <child link="${prefix}link_1"/>
        <origin xyz="0 0 0.450" rpy="0 0 0" />
        <axis xyz="0 0 1" />
        <limit lower="${radians(-180)}" upper="${radians(180)}" effort="7060.79" velocity="${radians(90)}"/>
    </joint>
    <joint name="${prefix}joint_2" type="revolute">
        <parent link="${prefix}link_1"/>
        <child link="${prefix}link_2"/>
        <origin xyz="0.740 0 0" rpy="0 0 0" />
        <axis xyz="0 1 0" />
        <limit lower="${radians(-130)}" upper="${radians(80)}" effort="11032.48" velocity="${radians(85)}"/>
    </joint>
    <joint name="${prefix}joint_3" type="revolute">
        <parent link="${prefix}link_2"/>
        <child link="${prefix}link_3"/>
        <origin xyz="1.150 0 0" rpy="0 0 0" />
        <axis xyz="0 -1 0" />
        <limit lower="${radians(-78.4)}" upper="${radians(78)}" effort="9929.23" velocity="${radians(85)}"/>
    </joint>
    <joint name="${prefix}joint_4" type="revolute">
        <parent link="${prefix}link_3"/>
        <child link="${prefix}link_4"/>
        <origin xyz="0.250 0 0" rpy="0 ${radians(90)} 0" />
        <axis xyz="-1 0 0" />
        <limit lower="${radians(-180)}" upper="${radians(180)}" effort="2621.3" velocity="${radians(120)}"/>
    </joint>
    <joint name="${prefix}joint_5" type="revolute">
        <parent link="${prefix}link_4"/>
        <child link="${prefix}link_5"/>
        <origin xyz="1.225 0 0" rpy="0 0 0" />
        <axis xyz="0 -1 0" />
        <limit lower="${radians(-125)}" upper="${radians(180)}" effort="3530.38" velocity="${radians(120)}"/>
    </joint>
    <joint name="${prefix}joint_6" type="revolute">
        <parent link="${prefix}link_5"/>
        <child link="${prefix}link_6"/>
        <origin xyz="0.250 0 0" rpy="0 0 0" />
        <axis xyz="1 0 0" />
        <limit lower="${radians(-180)}" upper="${radians(180)}" effort="1765.19" velocity="${radians(190)}"/>
    </joint>
    <!-- end of joint list -->

    <!-- ROS base_link to Robot Manufacturer World Coordinates transform -->
    <link name="${prefix}base" />
    <joint name="${prefix}base_link-base" type="fixed">
        <origin xyz="0 0 0.450" rpy="0 0 0"/>
        <parent link="${prefix}base_link"/>
        <child link="${prefix}base"/>
    </joint>

    <!-- ROS-Industrial 'flange' frame: attachment point for EEF models -->
    <link name="${prefix}flange"/>
    <joint name="${prefix}joint_6-flange" type="fixed">
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <parent link="${prefix}link_6"/>
      <child link="${prefix}flange"/>
    </joint>

    <!-- ROS-Industrial 'tool0' frame: all-zeros tool frame -->
    <link name="${prefix}tool0"/>
    <joint name="${prefix}flange-tool0" type="fixed">
      <origin xyz="0 0 0" rpy="${radians(180)} ${radians(-90)} 0"/>
      <parent link="${prefix}flange"/>
      <child link="${prefix}tool0"/>
    </joint>

</xacro:macro>
</robot>
