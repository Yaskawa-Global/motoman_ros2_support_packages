<?xml version="1.0" ?>
<robot xmlns:xacro="http://ros.org/wiki/xacro">
<xacro:macro name="motoman_motomini" params="prefix">
  <xacro:include filename="$(find motoman_resources)/urdf/common_materials.xacro"/>

  <!-- link list -->
  <link name="${prefix}base_link">
    <visual>
      <geometry>
        <mesh filename="package://motoman_motomini_support/meshes/motomini/visual/base_link.stl"/>
      </geometry>
      <xacro:material_yaskawa_blue/>
    </visual>
    <collision>
      <geometry>
        <mesh filename="package://motoman_motomini_support/meshes/motomini/collision/base_link.stl"/>
      </geometry>
    </collision>
  </link>
  <link name="${prefix}link_1">
    <visual>
      <geometry>
        <mesh filename="package://motoman_motomini_support/meshes/motomini/visual/link_1.stl"/>
      </geometry>
      <xacro:material_yaskawa_blue/>
    </visual>
    <collision>
      <geometry>
        <mesh filename="package://motoman_motomini_support/meshes/motomini/collision/link_1.stl"/>
      </geometry>
    </collision>
  </link>
  <link name="${prefix}link_2">
    <visual>
      <geometry>
        <mesh filename="package://motoman_motomini_support/meshes/motomini/visual/link_2.stl"/>
      </geometry>
      <xacro:material_yaskawa_blue/>
    </visual>
    <collision>
      <geometry>
        <mesh filename="package://motoman_motomini_support/meshes/motomini/collision/link_2.stl"/>
      </geometry>
    </collision>
  </link>
  <link name="${prefix}link_3">
    <visual>
      <geometry>
        <mesh filename="package://motoman_motomini_support/meshes/motomini/visual/link_3.stl"/>
      </geometry>
      <xacro:material_yaskawa_blue/>
    </visual>
    <collision>
      <geometry>
        <mesh filename="package://motoman_motomini_support/meshes/motomini/collision/link_3.stl"/>
      </geometry>
    </collision>
  </link>
  <link name="${prefix}link_4">
    <visual>
      <geometry>
        <mesh filename="package://motoman_motomini_support/meshes/motomini/visual/link_4.stl"/>
      </geometry>
      <xacro:material_yaskawa_blue/>
    </visual>
    <collision>
      <geometry>
        <mesh filename="package://motoman_motomini_support/meshes/motomini/collision/link_4.stl"/>
      </geometry>
    </collision>
  </link>
  <link name="${prefix}link_5">
    <visual>
      <geometry>
        <mesh filename="package://motoman_motomini_support/meshes/motomini/visual/link_5.stl"/>
      </geometry>
      <xacro:material_yaskawa_blue/>
    </visual>
    <collision>
      <geometry>
        <mesh filename="package://motoman_motomini_support/meshes/motomini/collision/link_5.stl"/>
      </geometry>
    </collision>
  </link>
  <link name="${prefix}link_6">
    <visual>
      <geometry>
        <mesh filename="package://motoman_motomini_support/meshes/motomini/visual/link_6.stl"/>
      </geometry>
      <xacro:material_yaskawa_blue/>
    </visual>
    <collision>
      <geometry>
        <mesh filename="package://motoman_motomini_support/meshes/motomini/collision/link_6.stl"/>
      </geometry>
    </collision>
  </link>
  <!-- end of link list -->

  <!-- joint list -->
  <joint name="${prefix}joint_1" type="revolute">
    <parent link="${prefix}base_link"/>
    <child link="${prefix}link_1"/>
    <origin xyz="0 0 0.103" rpy="0 0 0" />
    <axis xyz="0 0 1" />
    <limit lower="-2.9670" upper="2.9670" effort="0.12" velocity="5.4977"/>
  </joint>
  <joint name="${prefix}joint_2" type="revolute">
    <parent link="${prefix}link_1"/>
    <child link="${prefix}link_2"/>
    <origin xyz="0.020 0 0" rpy="0 0 0" />
    <axis xyz="0 1 0" />
    <limit lower="-1.4835" upper="1.5707" effort="0.12" velocity="5.4977"/>
  </joint>
  <joint name="${prefix}joint_3" type="revolute">
    <parent link="${prefix}link_2"/>
    <child link="${prefix}link_3"/>
    <origin xyz="0 0 0.165" rpy="0 0 0" />
    <axis xyz="0 -1 0" />
    <limit lower="-0.8726" upper="1.5707" effort="0.12" velocity="7.3304"/>
  </joint>
  <joint name="${prefix}joint_4" type="revolute">
    <parent link="${prefix}link_3"/>
    <child link="${prefix}link_4"/>
    <origin xyz="0.165 0 0" rpy="0 0 0" />
    <axis xyz="-1 0 0" />
    <limit lower="-2.4434" upper="2.4434" effort="0.07" velocity="10.4719"/>
  </joint>
  <joint name="${prefix}joint_5" type="revolute">
    <parent link="${prefix}link_4"/>
    <child link="${prefix}link_5"/>
    <origin xyz="0 0 0" rpy="0 0 0" />
    <axis xyz="0 -1 0" />
    <limit lower="-0.5235" upper="3.6651" effort="0.07" velocity="10.4719"/>
  </joint>
  <joint name="${prefix}joint_6" type="revolute">
    <parent link="${prefix}link_5"/>
    <child link="${prefix}link_6"/>
    <origin xyz="0 0 0" rpy="0 0 0" />
    <axis xyz="0 0 1" />
    <limit lower="-6.2831" upper="6.2831" effort="0.07" velocity="10.4719"/>
  </joint>
    <!-- end of joint list -->

    <!-- ROS base_link to Robot Manufacturer World Coordinates transform -->
    <link name="${prefix}base" />
    <joint name="${prefix}base_link-base" type="fixed">
      <origin xyz="0 0 0.103" rpy="0 0 0"/>
      <parent link="${prefix}base_link"/>
      <child link="${prefix}base"/>
    </joint>

  <!-- ROS-Industrial 'flange' frame: attachment point for EEF models -->
    <link name="${prefix}flange"/>
    <joint name="${prefix}joint_6-flange" type="fixed">
      <origin xyz="0 0 -0.040" rpy="0 ${pi/2} 0"/>
      <parent link="${prefix}link_6"/>
      <child link="${prefix}flange"/>
    </joint>

    <!-- ROS-Industrial 'tool0' frame: all-zeros tool frame -->
    <link name="${prefix}tool0"/>
    <joint name="${prefix}flange-tool0" type="fixed">
      <origin xyz="0 0 0 " rpy="${pi} ${-pi/2} 0"/>
      <parent link="${prefix}flange"/>
      <child link="${prefix}tool0"/>
    </joint>

  </xacro:macro>
</robot>

